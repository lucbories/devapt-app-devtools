{
	"services":{
		"authentication":{
			"type":"security",
			"routes":[
				{
					"route":"/login",
					"action":"login"
				},
				{
					"route":"/signin",
					"action":"login"
				},
				{
					"route":"/logout",
					"action":"logout"
				},
				{
					"route":"/signup",
					"action":"signup"
				},
				{
					"route":"/renew",
					"action":"renew"
				}
			]
		},

		"devtools_store":{
			"type":"html_assets",
			"routes":[
				{
					"route":"/assets2",
					"directory":"./public/tutorial-1",
					"default_file":"index.html"
				},
				{
					"route":"/examples",
					"directory":"./public/devtools",
					"default_file":"react_counter.html"
				}
			]
		},
		
		"metrics_http":{
			"type":"metrics_http",
			"browser":{
				"pollers":{
					"get": {"interval_seconds":3, "name":"metrics_http_poller"}
				},
				"timeline":{
					"get":[
						{
							"name":"requests_5m",
							"transform":"id_counter",
							"max":50,
							"interval_seconds":6
						},
						{
							"name":"requests_1h",
							"transform":"id_counter",
							"max":100,
							"interval_seconds":36
						},
						{
							"name":"requests_1d",
							"transform":"id_counter",
							"max":100,
							"interval_seconds":864
						},
						{
							"name":"latency_mean_5m",
							"transform":"latency_mean",
							"max":50,
							"interval_seconds":6
						},
						{
							"name":"latency_mean_1h",
							"transform":"latency_mean",
							"max":100,
							"interval_seconds":36
						},
						{
							"name":"latency_mean_1d",
							"transform":"latency_mean",
							"max":100,
							"interval_seconds":864
						}
					]
				}
			}
		},
		
		"metrics_host":{
			"type":"metrics_host"
		},
		
		"metrics_nodejs":{
			"type":"metrics_nodejs"
		},
		
		"metrics_bus":{
			"type":"metrics_bus"
		},
		
		"topology":{
			"type":"topology"
		},
		
		"logs":{
			"type":"logs"
		},
		
		"messages":{
			"type":"messages"
		},
		
		"devtools_panel":{
			"type":"middleware",
			"routes":[
				{
					"route":"/home",
					"page_view":"metrics_tabs",
					"page_menubar":"default_menubar"
				},
				{
					"route":"/topology/logical",
					"mw_file":"js/topology/topology_logical_mw.js"
				},
				{
					"route":"/topology/physical",
					"mw_file":"js/topology/topology_physical_mw.js"
				},
				{
					"route":"/metrics",
					"mw_file":"js/metrics/metrics_mw.js"
				},
				{
					"route":"/logs",
					"mw_file":"js/logs/logs_mw.js"
				},
				{
					"route":"/messages",
					"mw_file":"js/messages/messages_mw.js"
				},
                
                
				{
					"route":"/runtime/topology_registry",
					"mw_file":"js/runtime/topology_registry_mw.js"
				},
				{
					"route":"/runtime/topology_runtime",
					"mw_file":"js/runtime/topology_runtime_mw.js"
				},
				{
					"route":"/runtime/settings",
					"mw_file":"js/settings/settings_mw.js"
				},
				
				
				{
					"route":"/logs",
					"mw_file":"js/logs/logs_mw.js"
				},
				
				
				{
					"route":"/logs2",
					"redirect":"logs"
				}
			]
		},

		"html_assets_1":{
			"type":"html_assets",
			"routes":[
				{
					"is_global":true,
					"route":"/",
					"directory":"D:/DATAS/GitHub/devapt-app-devtools/dist/assets",
					"default":"index.html"
				},

				{
					"route":"/css/.*.css",
					"directory":"./public"
				},
				{
					"route":"/js",
					"directory":"public/assets/js"
				},
				{
					"route":"/img/.*.jpg",
					"directory":"./public"
				},
				{
					"route":"/plugins",
					"plugins":["Foundation-6", "Cytoscape", "Devtools", "Sparklines"]
				}
			],
			"security":{
				"enabled":false,
				"authentication":{
					"enabled":false,
					"engines":[]
				},
				"authorization":{
					"enabled":false,
					"engines":[],
					"permissions":{
						"*":["*"]
					}
				}
			}
		},
		
		"rest_api_models_query_1":{ 
			"type":"rest_api_models_query",
			"routes":[
				{
					"route":"/crud"
				}
			]
		},
		
		"rest_api_models_modifier_1":{
			"type":"rest_api_models_modifier",
			"routes":[
				{
					"route":"/crud"
				}
			]
		},
		
		"resources":{
			"type":"resources_query"
		},

		"rest_api_resources_query_1":{
			"type":"rest_api_resources_query",
			"routes":[
				{
					"route":"/connexions",
					"collection":"connexions"
				},
				{
					"route":"/connexions/:name",
					"collection":"connexions",
					"item":"name"
				},
				
				{
					"route":"/views",
					"collection":"views"
				},
				{
					"route":"/views/:name",
					"collection":"views",
					"item":"name"
				},
				
				{
					"route":"/models",
					"collection":"models"
				},
				{
					"route":"/models/:name",
					"collection":"models",
					"item":"name"
				},
				
				{
					"route":"/menubars",
					"collection":"menubars"
				},
				{
					"route":"/menubars/:name",
					"collection":"menubars",
					"item":"name"
				},
				
				{
					"route":"/menus",
					"collection":"menus"
				},
				{
					"route":"/menus/:name",
					"collection":"menus",
					"item":"name"
				}
			]
		}
	}
}