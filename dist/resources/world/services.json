{
	"services":{
		"authentication":{
			"type":"security",
			"routes":[
				{
					"route":"/login",
					"action":"login"
				},
				{
					"route":"/signin",
					"action":"login"
				},
				{
					"route":"/logout",
					"action":"logout"
				},
				{
					"route":"/signup",
					"action":"signup"
				},
				{
					"route":"/renew",
					"action":"renew"
				}
			]
		},

		"html_assets_1":{
			"type":"html_assets_svc",
			"routes":[
				{
					"route":"/plugins",
					"plugins":["Foundation6", "Labs", "Cytoscape", "Devtools", "Sparklines"]
				},
				{
					"is_global":true,
					"route":"$/^",
					"directory":"D:/DATAS/GitHub/devapt-app-devtools/dist/assets/",
					"default":"index.html"
				}
			],
			"security":{
				"enabled":false,
				"authentication":{
					"enabled":false,
					"engines":[]
				},
				"authorization":{
					"enabled":false,
					"engines":[],
					"permissions":{
						"*":["*"]
					}
				}
			}
		},
		
		"resources":{
			"type":"resources_svc",
			"routes":[
				{
					"route":"/connexions",
					"collection":"connexions"
				},
				{
					"route":"/connexions/:name",
					"collection":"connexions",
					"item":"name"
				},
				
				{
					"route":"/views",
					"collection":"views"
				},
				{
					"route":"/views/:name",
					"collection":"views",
					"item":"name"
				},
				
				{
					"route":"/models",
					"collection":"models"
				},
				{
					"route":"/models/:name",
					"collection":"models",
					"item":"name"
				},
				
				{
					"route":"/menubars",
					"collection":"menubars"
				},
				{
					"route":"/menubars/:name",
					"collection":"menubars",
					"item":"name"
				},
				
				{
					"route":"/menus",
					"collection":"menus"
				},
				{
					"route":"/menus/:name",
					"collection":"menus",
					"item":"name"
				}
			]
		}
	}
}